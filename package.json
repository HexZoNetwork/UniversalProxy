  "homepage": "https://github.com/HexZoNetwork/UniversalProxy#readme",
  "dependencies": {
    "ai-test-api": "file:TEST",
    "form-data": "^4.0.0",
    "node-fetch": "^2.6.1",
    "test": "^3.3.0"
  },
  "devDependencies": {
    "netlify-cli": "^17.10.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HexZoNetwork/UniversalProxy.git"
  },
  "bugs": {
    "url": "https://github.com/HexZoNetwork/UniversalProxy/issues"
